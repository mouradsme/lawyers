{
    "builds": [{
        "src": "static/wsgi.py",
        "use": "@natanffs/vercel-python-wsgi",
        "config": { 
            "maxLambdaSize": "15mb",
            "runtime": "python3.6"
            }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "static/wsgi.py"
        }
    ]
}